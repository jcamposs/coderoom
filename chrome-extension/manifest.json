{
  "name": "Screensharing CodeRoom",
  "description": "Screensharing utility for getscreenmedia",
  "version": "0.0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "icons": {
  },
  "permissions": [
    "desktopCapture"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [ {
    "js": [ "content.js" ],
    "matches": ["https://pre-coderoom.herokuapp.com/*",
                "http://127.0.0.1:3000/*",
                "http://localhost:3000/*"]
  }],
  "externally_connectable": {
    "matches": ["https://pre-coderoom.herokuapp.com/*",
                "http://127.0.0.1:3000/*",
                "http://localhost:3000/*"]
  }
}
